{
  "name": "nuxt-website-template",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.25.0",
  "description": "基于 Nuxt 4 的现代化开源官网模板",
  "author": "wolaishishihuo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wolaishishihuo/nuxt-website-template.git"
  },
  "keywords": [
    "nuxt",
    "nuxt4",
    "vue",
    "website",
    "template",
    "unocss",
    "typescript",
    "open-source"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
